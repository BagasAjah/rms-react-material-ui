{
  "name": "rms-react-app",
  "version": "0.1.4",
  "description": "Resource Management System React Application",
  "author": "Bagas Dimas",
  "license": "MIT",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start-server": "node server/server.js",
    "heroku-postbuild": "npm install",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --colors",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BagasAjah/rms-react-material-ui.git"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "~6.7.*",
    "babel-jest": "^18.0.0",
    "babel-loader": "~6.2.*",
    "babel-preset-es2015": "~6.6.*",
    "babel-preset-react": "~6.5.*",
    "babel-preset-stage-2": "^6.22.0",
    "jest": "^18.1.0",
    "react-scripts": "^0.8.5",
    "redux-devtools-extension": "^2.13.0",
    "ts-jest": "^18.0.3",
    "webpack": "~1.12.*",
    "webpack-dev-server": "~1.14.*"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "image-webpack-loader": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.16.6",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "react": "^15.0.0",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-search-input": "^0.10.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-ultimate-pagination": "^1.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^1.0.3"
  }
}
